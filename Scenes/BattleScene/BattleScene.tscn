[sub_resource type="Shader" id="Shader_la4u4"]
code = "shader_type canvas_item;

global uniform float EPSILON;

void fragment() {
	COLOR = texture(TEXTURE, UV);
	
	if (length(COLOR.rgb - vec3(0, 0, 0)) < EPSILON) {
		COLOR.rgb = vec3(0.2, 0.4, 1);
	}
}
"

